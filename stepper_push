#include <Stepper.h>

const int BUTTON = 4;
int BUTTONstate = 0;
const int stepsPerRevolution = 200;

Stepper myStepper(stepsPerRevolution, 8, 9, 10, 11);

void setup() {
  // put your setup code here, to run once:
  pinMode(BUTTON, INPUT);
  myStepper.setSpeed(60);
  Serial.begin(9600);
}

void loop() {
    Serial.print("hello")

}
